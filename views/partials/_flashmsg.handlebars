
{{#if flashMessenger }}
{{#each flashMessenger.alertsBeforeFlush}}
<div class="alert alert-{{this.type}} 
            {{#if this.canBeDismissed}}
                    alertdismissible
            {{/if}}>
            {{#if this.canBeDismissed}}
                <a href=" #" class="close" data-dismiss="alert" arialabel="close">&times;</a>
    {{/if}}
    <h6>
        {{#if this.titleIcon}}
        <i class="{{this.titleIcon}}"></i>
        {{/if}}
        {{this.title}}
    </h6>
    {{#if this.messages}}
    <ol>
        {{#each this.messages}}
        <li>{{this}}</li>
        {{/each}}
    </ol>
    {{/if}}
</div>
{{/each}}
{{/if}}